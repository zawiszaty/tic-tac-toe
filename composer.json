{
    "name": "zawiszaty/tic-tac-toe",
    "description": "tic tac toe game",
    "require": {
        "myclabs/php-enum": "^1.7",
        "symfony/flex": "^1.4",
        "symfony/console": "^4.3",
        "symfony/dependency-injection": "^4.3",
        "symfony/config": "^4.3",
        "symfony/yaml": "^4.3",
        "ext-readline": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpstan/phpstan": "^0.11.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
